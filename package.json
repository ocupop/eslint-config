{
  "name": "@ocupop/eslint-config",
  "version": "0.0.1",
  "engines": {
    "node": ">=10"
  },
  "description": "Ocupop ESLint default configuration",
  "bin": "setup.js",
  "main": "index.js",
  "repository": "https://github.com/ocupop/eslint-config",
  "author": "cdnicoll",
  "license": "MIT",
  "private": false,
  "scripts": {
    "checkFormat": "prettier --check '**/*.{js,jsx,ts,tsx,css,less,scss,graphql,yaml,yml,json,md}' ",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,less,scss,graphql,yaml,yml,json,md}' "
  },
  "devDependencies": {
    "@ocupop/prettier-config": "0.0.2",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "inquirer": "^7.1.0",
    "yargs": "^15.3.1"
  },
  "peerDependencies": {
    "eslint": ">= 6"
  }
}
